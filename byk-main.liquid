<div class="byk_main">
    {% render 'close-modal' %}
        {% if section.settings.button_label != blank %}
        <div>
            <button id="openModal" class="product-form__submit button button--primary">{{ section.settings.button_label }}</button>
        </div>
        {% endif %}
        
    <div class="modal">
        <div style="display:block;" class="modal-overlay"></div>
        <div class="modal-wrapper modal-transition">
        <div class="modal-header">
            <button class="modal-close modal-toggle"><svg class="icon-close icon" viewBox="0 0 32 32"><use xlink:href="#icon-close"></use></svg></button>
            <h2 class="modal-heading">BMW & Porsche Oil Change Specifications</h2>
        </div>
        
        <div class="modal-body">
            <div class="modal-content">
                <div class="halo-text-format">
                    <div class="main_accordion_container">
                    <div class="heading_accordion_bmw">
                        BMW Oil Change Specifications
                    </div>
                    <div class="accordion_content_bmw">
                        {% for engine in page.metafields.custom.bmw_oil_change_specifications.value %}
                            {% assign engine_oil = engine.engine_oil | strip %}
                            <details class="accordion_item">
                                <summary class="accordion_head">{{ engine.name }} ({{ engine.years }})</summary>
                                <div class="byk_product_modal" id="modal_{{ engine.id }}">
                                <div style="display: block;" class="modal-overlay_modal"></div>
                                <div style="background:#fff" class="byk_product_modal_wrapper">
                                    <div class="modal-header_modal">
                                        <button class="modal-close modal-toggles" data-modal-id="{{ engine.id }}" >
                                            <svg class="icon-close icon" viewBox="0 0 32 32"><use xlink:href="#icon-close"></use></svg>
                                        </button>
                                    </div>

                                    {% for product in section.settings.product_list_modal %}
                                        <div class="bundle_item_modal {% if product.available == false %}product_out_of_stock{% endif %}">
                                            <div class="bundle_item_container">
                                            <div class="bundle_check">
                                                <input 
                                                    data-variant-modalqty="1" 
                                                    {% if product.available == false %} disabled {% endif %}
                                                    {% if product.available == true %} data-qty="bundle_variant_qty-{{ forloop.index }}" {% endif %} 
                                                    {% if product.available == true %}
                                                        data-checkoption="checkbox_option_bundle_modal_{{ forloop.index  }}_{{ engine.id }}_quantity" 
                                                        data-dropdown="bundle_variant_select-{{ forloop.index }}"
                                                    {% endif %} 
                                                    type="checkbox" 
                                                    {% for bundle_variant in product.variants %}
                                                        {% assign variant_title = bundle_variant.title | strip %}
                                                        {% if variant_title == engine_oil %}
                                                        value="{{ bundle_variant.id }}"
                                                        {% endif %}
                                                    {% endfor %}
                                                    style="display:block !important;" 
                                                    class="bundle_checkbox_optionBYK_modal checkbox_option_bundle-modal-{{ forloop.index }}_{{ engine.id }} {% if product.variants.size > 1 %}have_variant{% endif %}"  
                                                />
                                            </div>

                                            <div class="bundle_product_image">
                                                <img class="bundle_image" src="{{ product.featured_media | img_url: '100x100'}}"/>
                                            </div>

                                            <div class="bundle_content " >
                                                <label for="checkbox_option_bundle-{{ forloop.index }}">
                                                    <div class="bundle_title">
                                                        <p class="bundle_product_title" style="margin:0">{{ product.title }}</p>
                                                        <span class="preview_price-{{ forloop.index }}_{{ engine.id }}">
                                                        {% for bundle_variant in product.variants %}
                                                            {% assign variant_title = bundle_variant.title | strip %}
                                                            {% if variant_title == engine_oil %}
                                                                {{ bundle_variant.price |  money }}
                                                            {% endif %}
                                                        {% endfor %}   
                                                        </span>
                                                    </div>
                                                </label>
                                                {{engine_oil}}
                                            </div>
                                            </div>
                                        
                                            <div class="quantity_bundle">
                                            <div class="quantity_selector">
                                                {% render 'product-quantity-variant-modal',
                                                    id_field: forloop.index ,
                                                    modal_id: engine.id,
                                                    current_variant: product.selected_or_first_available_variant.id ,
                                                    qty_style: '1',
                                                    quantity_label: product.metafields.custom.quantity_label,
                                                    current_price: product.selected_or_first_available_variant.price,
                                                    check_hidden: false,
                                                    quantity_stt: 2
                                                %}
                                            </div> 
                                            </div>
                                        </div>
                                    {% endfor %}

                                    <div class="modal-header_modal">
                                        <button type="button" class="byk_add_btn_modal_btn product-form__submit button button--primary">
                                            ADD ITEMS TO CART
                                        </button>
                                    </div>
                                </div>
                                </div>

                                <div class="ac_content">
                                <table border="1">
                                    <tbody>
                                        <tr>
                                            <th>Year</th>
                                            <th>Engine Oil</th>
                                            <th>Capacity</th>
                                            <th>Oil Filter</th>
                                        </tr>
                                        <tr>
                                            <td>({{ engine.years }})</td>
                                            <td class="engine_oil_container">{{ engine.engine_oil }} 
                                            <button type="button" class="open_modal_cart " data-modal-id="{{ engine.id }}" >
                                                {% render 'cart-icon' %}
                                            </button>
                                            </td>
                                            <td>{{ engine.capacity }}</td>
                                            <td class="oil_filter_container" >
                                            {{ engine.oil_filter }}  
                                            <button type="button" 
                                                {% for product_item in section.settings.product_oil_filter %}
                                                    {% for variant_item in product_item.variants  %}
                                                        {% if variant_item.title == engine.oil_filter %}
                                                        {%  assign variant_item_id = variant_item.id%} 
                                                        {% endif %}
                                                    {% endfor %}
